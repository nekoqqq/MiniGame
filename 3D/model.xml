<root>
	<VertexBuffer name="player">
		<!-- +Z面 (法线方向 0,0,1) -->
		<Vertex position="10,10,10" uv="0.0,0.0" color="0xFFFF0000"></Vertex>
		<Vertex position="-10,10,10" uv="1.0,0.0" color="0xFFFF0000"></Vertex>
		<Vertex position="-10,0,10" uv="1.0,1.0" color="0xFFFF0000"></Vertex>
		<Vertex position="10,0,10" uv="0.0,1.0" color="0xFFFF0000"></Vertex>

		<!-- -Z面 (法线方向 0,0,-1) -->
		<Vertex position="-10,10,-10" uv="0.0,0.0" color="0xFF00FF00"></Vertex>
		<Vertex position="10,10,-10" uv="1.0,0.0" color="0xFF00FF00"></Vertex>
		<Vertex position="10,0,-10" uv="1.0,1.0" color="0xFF00FF00"></Vertex>
		<Vertex position="-10,0,-10" uv="0.0,1.0" color="0xFF00FF00"></Vertex>

		<!-- -X面 (法线方向 -1,0,0) -->
		<Vertex position="-10,10,10" uv="0.0,0.0" color="0xFF0000FF"></Vertex>
		<Vertex position="-10,10,-10" uv="1.0,0.0" color="0xFF0000FF"></Vertex>
		<Vertex position="-10,0,-10" uv="1.0,1.0" color="0xFF0000FF"></Vertex>
		<Vertex position="-10,0,10" uv="0.0,1.0" color="0xFF0000FF"></Vertex>

		<!-- +X面 (法线方向 1,0,0) -->
		<Vertex position="10,10,-10" uv="0.0,0.0" color="0xFFFFFFFF"></Vertex>
		<Vertex position="10,10,10" uv="1.0,0.0" color="0xFFFFFFFF"></Vertex>
		<Vertex position="10,0,10" uv="1.0,1.0" color="0xFFFFFFFF"></Vertex>
		<Vertex position="10,0,-10" uv="0.0,1.0" color="0xFFFFFFFF"></Vertex>

		<!-- +Y面 (法线方向 0,1,0) -->
		<Vertex position="-10,10,10" uv="0.0,0.0" color="0xFF4F0000"></Vertex>
		<Vertex position="10,10,10" uv="1.0,0.0" color="0xFF4F0000"></Vertex>
		<Vertex position="10,10,-10" uv="1.0,1.0" color="0xFF00004F"></Vertex>
		<Vertex position="-10,10,-10" uv="0.0,1.0" color="0xFF00004F"></Vertex>

		<!-- -Y面 (法线方向 0,-1,0) -->
		<Vertex position="10,0,10" uv="0.0,0.0" color="0xFF000000"></Vertex>
		<Vertex position="-10,0,10" uv="1.0,0.0" color="0xFF000000"></Vertex>
		<Vertex position="-10,0,-10" uv="1.0,1.0" color="0xFF000000"></Vertex>
		<Vertex position="10,0,-10" uv="0.0,1.0" color="0xFF000000"></Vertex>
	</VertexBuffer>


	<VertexBuffer name="stage">
		<Vertex position="-5000,0,-5000" uv="0.0,0.0" color="0xffffffff"></Vertex>
		<Vertex position="-5000,0,5000" uv="0.0,1.0" color="0xffffffff"></Vertex>
		<Vertex position="5000,0,-5000" uv="1.0,0.0" color="0xffffffff"></Vertex>
		<Vertex position="5000,0,5000" uv="1.0,1.0" color="0xffffffff"></Vertex>
	</VertexBuffer>
	<VertexBuffer name="axis">
		<!-- x轴 -->
		<Vertex position="-100,1,0" uv="0.0,0.0" color="0xffff0000"></Vertex>
		<Vertex position="100,1,0" uv="0.0,0.0" color="0xffff0000"></Vertex>
		<Vertex position="100,-1,0" uv="0.0,0.0" color="0xffff0000"></Vertex>
		<Vertex position="-100,-1,0" uv="0.0,0.0" color="0xffff0000"></Vertex>

		<!-- y轴 -->
		<Vertex position="-1,-100,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>
		<Vertex position="1,-100,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>
		<Vertex position="1,100,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>
		<Vertex position="-1,100,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>

		<!-- z轴 -->
		<Vertex position="-1,0,100" uv="0.0,0.0" color="0xff0000ff"></Vertex>
		<Vertex position="1,0,100" uv="0.0,0.0" color="0xff0000ff"></Vertex>
		<Vertex position="1,0,-100" uv="0.0,0.0" color="0xff0000ff"></Vertex>
		<Vertex position="-1,0,-100" uv="0.0,0.0" color="0xff0000ff"></Vertex>

		<!-- x箭头 -->
		<Vertex position="100,5,0" uv="0.0,0.0" color="0xffff0000"></Vertex>
		<Vertex position="120,0,0" uv="0.0,0.0" color="0xffff0000"></Vertex>
		<Vertex position="100,-5,0" uv="0.0,0.0" color="0xffff0000"></Vertex>

		<!-- y箭头 -->
		<Vertex position="-5,100,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>
		<Vertex position="5,100,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>
		<Vertex position="0,120,0" uv="0.0,0.0" color="0xff00ff00"></Vertex>

		<!-- z箭头 -->
		<Vertex position="0,5,100" uv="0.0,0.0" color="0xff0000ff"></Vertex>
		<Vertex position="0,-5,100" uv="0.0,0.0" color="0xff0000ff"></Vertex>
		<Vertex position="0,0,120" uv="0.0,0.0" color="0xff0000ff"></Vertex>
	</VertexBuffer>
	<VertexBuffer name="wall">
		<Vertex position="-200, 0, -200" uv="0, 0"></Vertex>
		<Vertex position="-200, 0, 200" uv="0, 1"></Vertex>
		<Vertex position="-200, 150, 200" uv="1, 0"></Vertex>
		<Vertex position="-200, 150, -200" uv="1, 1"></Vertex>

		<Vertex position="-100, 0, -100" uv="0, 0"></Vertex>
		<Vertex position="-100, 0, 300" uv="0, 1"></Vertex>
		<Vertex position="-100, 150, 300" uv="1, 0"></Vertex>
		<Vertex position="-100, 150, -100" uv="1, 1"></Vertex>




	</VertexBuffer>
	<VertexBuffer name="missle">
		<!-- 头部顶点（锥尖） -->
		<Vertex position="0, 0, 0" uv="0.5,0.5" color="0xFFFF0000"></Vertex>
		<!-- 红色头部 -->

		<!-- 尾部环形顶点（Z轴正方向延伸） -->
		<Vertex position="-5, -5, -80" uv="0.0,1.0" color="0xFF00FF00"></Vertex>
		<!-- 绿色尾翼1 -->
		<Vertex position="5, -5, -80" uv="1.0,1.0" color="0xFF00FF00"></Vertex>
		<!-- 绿色尾翼2 -->
		<Vertex position="0, 5, -80" uv="0.5,0.0" color="0xFF0000FF"></Vertex>
		<!-- 蓝色尾翼顶部 -->
		<Vertex position="0, -5, -80" uv="0.5,1.0" color="0xFFFFFF00"></Vertex>
		<!-- 黄色底部 -->
	</VertexBuffer>


	<IndexBuffer name="player">
		<!-- +Z面 -->
		<Triangle indices="0,1,2"></Triangle>
		<Triangle indices="0,2,3"></Triangle>

		<!-- -Z面 -->
		<Triangle indices="4,5,6"></Triangle>
		<Triangle indices="4,6,7"></Triangle>

		<!-- -X面 -->
		<Triangle indices="8,9,10"></Triangle>
		<Triangle indices="8,10,11"></Triangle>

		<!-- +X面 -->
		<Triangle indices="12,13,14"></Triangle>
		<Triangle indices="12,14,15"></Triangle>

		<!-- +Y面 -->
		<Triangle indices="16,17,18"></Triangle>
		<Triangle indices="16,18,19"></Triangle>

		<!-- -Y面 -->
		<Triangle indices="20,21,22"></Triangle>
		<Triangle indices="20,22,23"></Triangle>
	</IndexBuffer>
	<IndexBuffer name="stage">
		<Triangle indices="0, 1, 2"></Triangle>
		<Triangle indices="3, 2, 1"></Triangle>
	</IndexBuffer>
	<IndexBuffer name="axis">
		<Triangle indices="0, 1, 2"></Triangle>
		<Triangle indices="3, 2, 0"></Triangle>

		<Triangle indices="4, 5, 6"></Triangle>
		<Triangle indices="7, 6, 4"></Triangle>

		<Triangle indices="8, 9, 10"></Triangle>
		<Triangle indices="11, 10, 8"></Triangle>

		<Triangle indices="12, 13, 14"></Triangle>
		<Triangle indices="15, 16, 17"></Triangle>
		<Triangle indices="18, 19, 20"></Triangle>
	</IndexBuffer>
	<IndexBuffer name="wall">
		<!-- +z面 -->
		<Triangle indices="0, 1, 2"></Triangle>
		<Triangle indices="3, 2, 0"></Triangle>

		<!-- -z面 -->
		<Triangle indices="4, 5, 6"></Triangle>
		<Triangle indices="7, 6, 4"></Triangle>



	</IndexBuffer>
	<IndexBuffer name="missle">
		<!-- 锥形头部侧面 -->
		<Triangle indices="0,1,2"></Triangle>
		<!-- 头部到左尾翼 -->
		<Triangle indices="0,2,3"></Triangle>
		<!-- 头部到右尾翼 -->
		<Triangle indices="0,3,4"></Triangle>
		<!-- 头部到底部 -->

		<!-- 尾翼平面 -->
		<Triangle indices="1,2,3"></Triangle>
		<!-- 左尾翼到右尾翼到顶部 -->
		<Triangle indices="1,3,4"></Triangle>
		<!-- 左尾翼到顶部到底部 -->
		<Triangle indices="2,3,4"></Triangle>
		<!-- 右尾翼到顶部到底部 -->
	</IndexBuffer>
	
	<Texture name="player" file_name="player.tga"></Texture>
	<Texture name="stage" file_name="stage.tga"></Texture>


	<Painter name="player" vertexBuffer="player" indexBuffer="player" texture="" blend="opaque" origin="0.0,0.1,0.0"></Painter>
	<Painter name="enemy" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="20.0,50.0,30.0"></Painter>
	<Painter name="enemy_1" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,30.0"></Painter>
	<Painter name="enemy_2" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,20.0"></Painter>
	<Painter name="enemy_3" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,30.0"></Painter>
	<Painter name="enemy_4" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,40.0"></Painter>
	<Painter name="enemy_5" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,50.0"></Painter>
	<Painter name="enemy_6" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,60.0"></Painter>
	<Painter name="enemy_7" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,70.0"></Painter>
	<Painter name="enemy_8" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,80.0"></Painter>
	<Painter name="enemy_9" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,90.0"></Painter>
	<Painter name="enemy_10" vertexBuffer="player" indexBuffer="player" blend="opaque" origin="0.0,0.0,100.0"></Painter>

	<Painter name="stage" vertexBuffer="stage" indexBuffer="stage" texture="stage" blend="opaque"></Painter>
	<Painter name="wall" vertexBuffer="wall" indexBuffer="wall" texture="stage" blend="opaque"></Painter>

	<Painter name="axis" vertexBuffer="axis" indexBuffer="axis" texture="axis" blend="opaque"></Painter>
	<Painter name="missle" vertexBuffer="missle" indexBuffer="missle" texture="" blend="opaque"></Painter>

	<TransformTree name="SolarSystem">
		<Node name="SunRotation" painter="player">
			<Node name="EarthTranslation" translation="100.0,0.0,0.0">
				<Node name="EarthRotation" painter="player"></Node>
				<Node name="MoonRotation">
					<Node name="MoonTranslation" painter="player" translation="30.0,0.0,0.0"></Node>
				</Node>
			</Node>
		</Node>
	</TransformTree>
	
	<Animation name="SolarSystem">
		<Node name="SunRotation">
			<Curve type="rotation_y" interpolation="cubic">
				<Data time="0" value="0"></Data>
				<Data time="1200" value="-120"></Data>
				<Data time="2400" value="-240"></Data>
				<Data time="3650" value="-360"></Data>
			</Curve>
		</Node>
		<Node name="MoonRotation">
			<Curve type="rotation_y" interpolation="cubic">
				<Data time="0" value="0"></Data>
				<Data time="100" value="-120"></Data>
				<Data time="200" value="-240"></Data>
				<Data time="300" value="-360"></Data>
			</Curve>
		</Node>
		<Node name="EarthRotation">
			<Curve type="rotation_y" interpolation="cubic">
				<Data time="0" value="0"></Data>
				<Data time="10" value="-120"></Data>
				<Data time="20" value="-240"></Data>
				<Data time="30" value="-360"></Data>
			</Curve>
		</Node>
	</Animation>
</root>